{
    "name": "mhamlet/laravel-apidocs",
    "description": "API Documentation generator for Laravel",
    "keywords": [
        "mhamlet",
        "laravel",
        "laravel-apidocs",
        "apidocs",
        "documentation",
        "api",
        "generate"
    ],
    "homepage": "https://github.com/mhamlet/laravel-apidocs",
    "license": "MIT",
    "authors": [
        {
            "name": "Hamlet Muradyan",
            "email": "contact@hamletmuradyan.com",
            "homepage": "https://hamletmuradyan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "5.4.*",
        "illuminate/routing": "5.4.*",
        "php" : ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "MHamlet\\Apidocs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MHamlet\\Apidocs\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
